    {[...Array(product.countInStock).keys()].map((x) => (\n                            <option key={x + 1} value={x + 1}>\n                              {x + 1}\n                            </option>\n                          ))}\n                        </Form.Control>\n                      </Col>\n                    </Row>\n                  </ListGroup.Item>\n                )}\n\n                <ListGroup.Item>\n                  <Button\n                    onClick={addToCartHandler}\n                    className='btn-block'\n                    type='button'\n                    disabled={product.countInStock === 0}\n                  >\n                    Add To Cart\n                  </Button>\n                </ListGroup.Item>\n              </ListGroup>\n            </Card>\n          </Col>\n        </Row>\n      )}\n    </>\n  )\n}\n\nexport default ProductScreen\n","/home/ling/MERN/e-commerce/frontend/src/products.js",[],"/home/ling/MERN/e-commerce/frontend/src/reducers/productReducers.js",[],"/home/ling/MERN/e-commerce/frontend/src/actions/productActions.js",[],"/home/ling/MERN/e-commerce/frontend/src/constants/productConstants.js",[],"/home/ling/MERN/e-commerce/frontend/src/store.js",[],["80","81"],"/home/ling/MERN/e-commerce/frontend/src/reducers/cartReducers.js",[],"/home/ling/MERN/e-commerce/frontend/src/components/Loader.js",[],"/home/ling/MERN/e-commerce/frontend/src/screens/CartScreen.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"82","replacedBy":"86"},{"ruleId":"84","replacedBy":"87"},{"ruleId":"88","severity":1,"message":"89","line":6,"column":8,"nodeType":"90","messageId":"91","endLine":6,"endColumn":13},{"ruleId":"82","replacedBy":"92"},{"ruleId":"84","replacedBy":"93"},"no-native-reassign",["94"],"no-negated-in-lhs",["95"],["94"],["95"],"no-unused-vars","'axios' is defined but never used.","Identifier","unusedVar",["94"],["95"],"no-global-assign","no-unsafe-negation"]