Message variant='danger'>{error}</Message>\n      ) : (\n        <Row>\n          <Col md={6}>\n            <Image src={product.image} alt={product.name} fluid />\n          </Col>\n          <Col md={3}>\n            <ListGroup variant='flush'>\n              <ListGroup.Item>\n                <h3>{product.name}</h3>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Rating\n                  value={product.rating}\n                  text={`${product.numReviews} reviews`}\n                />\n              </ListGroup.Item>\n              <ListGroup.Item>Price: ${product.price}</ListGroup.Item>\n              <ListGroup.Item>\n                Description: {product.description}\n              </ListGroup.Item>\n            </ListGroup>\n          </Col>\n          <Col md={3}>\n            <Card>\n              <ListGroup variant='flush'>\n                <ListGroup.Item>\n                  <Row>\n                    <Col>Price:</Col>\n                    <Col>\n                      <strong>${product.price}</strong>\n                    </Col>\n                  </Row>\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                  <Row>\n                    <Col>Status:</Col>\n                    <Col>\n                      {product.countInStock > 0 ? 'In Stock' : 'Out Of Stock'}\n                    </Col>\n                  </Row>\n                </ListGroup.Item>\n\n                {product.countInStock > 0 && (\n                  <ListGroup.Item>\n                    <Row>\n                      <Col>Qty</Col>\n                      <Col>\n                        <Form.Control\n                          as='select'\n                          value={qty}\n                          onChange={(e) => setQty(e.target.value)}\n                        >\n                          {[...Array(product.countInStock).keys()].map((x) => (\n                            <option key={x + 1} value={x + 1}>\n                              {x + 1}\n                            </option>\n                          ))}\n                        </Form.Control>\n                      </Col>\n                    </Row>\n                  </ListGroup.Item>\n                )}\n\n                <ListGroup.Item>\n                  <Button\n                    onClick={addToCartHandler}\n                    className='btn-block'\n                    type='button'\n                    disabled={product.countInStock === 0}\n                  >\n                    Add To Cart\n                  </Button>\n                </ListGroup.Item>\n              </ListGroup>\n            </Card>\n          </Col>\n        </Row>\n      )}\n    </>\n  )\n}\n\nexport default ProductScreen\n","/home/minmaw/workspace/e-commerce_MERN/frontend/src/constants/productConstants.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/actions/cartActions.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/constants/userConstants.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/components/FormContainer.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/screens/LoginScreen.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/components/Loader.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/components/Header.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/actions/userActions.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/screens/CartScreen.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/components/Message.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/constants/cartConstants.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/reducers/userReducers.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/screens/RegisterScreen.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/App.js",[],"/home/minmaw/workspace/e-commerce_MERN/frontend/src/screens/ProfileScreen.js",[],{"ruleId":"99","replacedBy":"100